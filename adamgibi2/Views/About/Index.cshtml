@using adamgibi2.Models.Siniflar
@model List<Hakkimizda>
@{
    ViewData["Title"] = "About";
    Layout = "/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>About</h3>
           
            @foreach (var x in Model)
            {
                <img src="@x.FotoUrl" alt="" />
                <p>@x.Aciklama</p>
            }
        </div>
    </div>
</div>